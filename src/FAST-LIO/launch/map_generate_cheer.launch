<launch>
<!-- Launch file for Livox MID 70 LiDAR -->
        <param name="debug" type="bool" value="true" />
	<param name="optimal" type="bool" value="true" />
	<param name="optimal_deque_size" type="int" value="5" />
	
	<param name="stable_max_x" type="double" value="5.0" />
	<param name="stable_min_x" type="double" value="0.5" />
	<param name="stable_max_y" type="double" value="1.0" />
	<param name="stable_min_y" type="double" value="-0.3" />
	<param name="stable_max_z" type="double" value="3.0" />
	<param name="stable_min_z" type="double" value="0.2" />
	
	<param name="moving_max_x" type="double" value="5.0" />
	<param name="moving_min_x" type="double" value="0.5" />
	<param name="moving_max_y" type="double" value="-1.0" />
	<param name="moving_min_y" type="double" value="-2.5" />
	<param name="moving_max_z" type="double" value="3.0" />
	<param name="moving_min_z" type="double" value="0.2" />
	
	<param name="diff_radius" type="double" value="0.05" />
	<param name="min_radius" type="double" value="0.2" />
	<param name="max_radius" type="double" value="1.5" />
	<param name="cylinder_normal_weight" type="double" value="0.1" />
	<param name="cylinder_distance_threshold" type="double" value="0.05" />
	<param name="plane_normal_weight" type="double" value="0.5" />
	<param name="plane_distance_threshold" type="double" value="0.02" />
    	<node pkg="fast_lio" type="map_generate" name="map_generate" output="screen" /> 
    	<arg name="rviz" default="true" />
    	<group if="$(arg rviz)">
	<node launch-prefix="nice" pkg="rviz" type="rviz" name="rviz" args="-d $(find fast_lio)/rviz_cfg/3d_bbox.rviz" />
	</group>
</launch>
