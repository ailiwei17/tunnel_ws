	<param name="min_x" type="double" value="0.5" />
	<param name="max_y" type="double" value="1.0" />
	<param name="min_y" type="double" value="-0.3" />
	<param name="max_z" type="double" value="3.0" />
	<param name="min_z" type="double" value="0.2" />
	<param name="voxel_size_x" type="double" value="0.5" />
